            <URL value="@../Css/login.css" />
          </stylesheets>
        </Button>
      </children>
      <stylesheets>
        <URL value="@../Css/login.css" />
      </stylesheets>
    </AnchorPane>
    <Label alignment="CENTER" layoutX="254.0" layoutY="25.0" prefHeight="55.0" prefWidth="192.0" text="Sign Up" textFill="WHITE">
      <font>
        <Font size="24.0" />
      </font>
    </Label>
  </children>
  <stylesheets>
    <URL value="@../Css/login.css" />
  </stylesheets>
